load("@rules_proto//proto:defs.bzl", "proto_library")
load("@build_stack_rules_proto//rules/proto:proto_compile_test.bzl", "proto_compile_test")
load("//bazel/python:defaults.bzl", "py_library", "py_rstubs_proto_compile")

# gazelle:proto package
# gazelle:protoc_gen_rstubs service.proto
# gazelle:proto_language py

proto_library(
    name = "service_proto",
    srcs = ["service.proto"],
    visibility = ["//visibility:public"],
)

py_rstubs_proto_compile(
    name = "service_py_rstubs_proto_compile",
    deps = [":service_proto"],
)

proto_compile_test(
    name = "service_py_rstubs_proto_compile_test",
    srcs = ["service.py"],
    rule = ":service_py_rstubs_proto_compile",
    visibility = ["//visibility:private"],
)

py_library(
    name = "service_py_library",
    srcs = ["service.py"],
    imports = [".."],
)
